{
    "name": "git-visualize",
    "type": "module",
    "version": "0.1.5",
    "description": "A CLI tool to visualize git repositories.",
    "homepage": "https://github.com/acandoo/enodo/tree/main/packages/git-visualize#readme",
    "bugs": {
        "url": "https://github.com/acandoo/enodo/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/acandoo/enodo.git",
        "directory": "packages/git-visualize"
    },
    "bin": {
        "git-visualize": "./dist/index.js"
    },
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "dev": "tsx src/index.ts",
        "build": "pkgroll --minify",
        "start": "node dist/index.js",
        "test": "tsx --test"
    },
    "keywords": [
        "git",
        "chart",
        "analyze",
        "data science",
        "cli",
        "repository"
    ],
    "author": "acandoo",
    "license": "ISC",
    "devDependencies": {
        "pkgroll": "^2.15.3",
        "tsx": "^4.20.4"
    },
    "dependencies": {
        "@commander-js/extra-typings": "^14.0.0",
        "@inquirer/confirm": "^5.1.15",
        "@observablehq/plot": "^0.6.17",
        "@types/cli-progress": "^3.11.6",
        "@types/jsdom": "^21.1.7",
        "chart.js": "^4.5.0",
        "cli-progress": "^3.12.0",
        "commander": "^14.0.0",
        "isomorphic-git": "^1.33.0",
        "jsdom": "^26.1.0",
        "puppeteer": "^24.17.0",
        "skia-canvas": "^2.0.2"
    }
}
